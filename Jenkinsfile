pipeline {
    agent any
    stages {
        stage('1st stage'){
            steps {
                echo 'Hello '
                sh 'ls'
                }
            }
        stage('2nd stage'){
            steps {
                echo 'Jenkinsfile'
                sh 'date'
                }
            }
        stage('3rd stage'){
            steps{
                echo 'this is part of jenkins lab 1 solution'
            }
        }
    }
}
